{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".nuxt/**", ".output/**"]
    },
    "test": {
      "outputs": []
    },
    "lint": {
      "outputs": []
    },
    "stub": {
      "cache": false
    },
    "dev": {
      "dependsOn": ["config#stub"],
      "cache": false
    },
    "start": {
      "dependsOn": ["build"]
    },
    "typecheck": {
      "outputs": []
    }
  },
  "globalDependencies": ["pnpm-lock.yaml"]
}
